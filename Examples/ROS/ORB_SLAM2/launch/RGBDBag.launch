<launch>

<arg name="r" default="1"/>

<arg name="bag" default="fr2"/>

<param name="bag" type="string" value="$(arg bag)" />
<param name="rate" type="string" value="$(arg r)" />

<node pkg="rosbag" type="play" name="player" output="screen" args="--clock --pause -r $(arg r)  /home/andre/Documents/bags/$(arg bag).bag /imu0:=/imu/data_raw /camera/rgb/image_color:=/camera/rgb/image_raw /camera/depth/image:=/camera/depth_registered/image_raw"/>

<param name = "/use_sim_time" value="true"/>

</launch>
